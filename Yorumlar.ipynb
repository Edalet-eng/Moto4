    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "# Veritabanı bağlantısı oluştur\n",
    "conn = sqlite3.connect(\"yorumlar.db\")\n",
    "\n",
    "# Yorumları alın ve bir Pandas DataFrame'e yükleyin\n",
    "yorumlar = pd.read_sql(\"SELECT * FROM yorumlar\", conn)\n",
    "\n",
    "yorumlar\n"
